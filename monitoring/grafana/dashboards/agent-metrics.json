{
  "dashboard": {
    "id": null,
    "title": "HauntedAI Agent Metrics",
    "tags": ["hauntedai", "agents"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Agent Execution Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(hauntedai_agent_executions_total[5m])",
            "legendFormat": "Executions/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            },
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Agent Failure Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(hauntedai_agent_failures_total[5m])",
            "legendFormat": "Failures/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.01},
                {"color": "red", "value": 0.05}
              ]
            },
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Rooms Created",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(hauntedai_rooms_created_total[5m])",
            "legendFormat": "Rooms/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.1},
                {"color": "red", "value": 1}
              ]
            },
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(hauntedai_agent_executions_total{status=\"success\"}[5m]) / rate(hauntedai_agent_executions_total[5m]) * 100",
            "legendFormat": "Success Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 80},
                {"color": "green", "value": 95}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Agent Executions by Type",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(hauntedai_agent_executions_total[5m])",
            "legendFormat": "{{agent_type}} ({{status}})"
          }
        ],
        "yAxes": [
          {
            "label": "Executions/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Agent Failures by Type",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(hauntedai_agent_failures_total[5m])",
            "legendFormat": "{{agent_type}} ({{error_type}})"
          }
        ],
        "yAxes": [
          {
            "label": "Failures/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "StoryAgent Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(hauntedai_agent_executions_total{agent_type=\"story\"}[5m])",
            "legendFormat": "Story Executions"
          },
          {
            "expr": "rate(hauntedai_agent_failures_total{agent_type=\"story\"}[5m])",
            "legendFormat": "Story Failures"
          }
        ],
        "yAxes": [
          {
            "label": "Rate/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 9, "w": 6, "x": 0, "y": 17}
      },
      {
        "id": 8,
        "title": "AssetAgent Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(hauntedai_agent_executions_total{agent_type=\"asset\"}[5m])",
            "legendFormat": "Asset Executions"
          },
          {
            "expr": "rate(hauntedai_agent_failures_total{agent_type=\"asset\"}[5m])",
            "legendFormat": "Asset Failures"
          }
        ],
        "yAxes": [
          {
            "label": "Rate/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 9, "w": 6, "x": 6, "y": 17}
      },
      {
        "id": 9,
        "title": "CodeAgent Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(hauntedai_agent_executions_total{agent_type=\"code\"}[5m])",
            "legendFormat": "Code Executions"
          },
          {
            "expr": "rate(hauntedai_agent_failures_total{agent_type=\"code\"}[5m])",
            "legendFormat": "Code Failures"
          }
        ],
        "yAxes": [
          {
            "label": "Rate/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 9, "w": 6, "x": 12, "y": 17}
      },
      {
        "id": 10,
        "title": "DeployAgent Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(hauntedai_agent_executions_total{agent_type=\"deploy\"}[5m])",
            "legendFormat": "Deploy Executions"
          },
          {
            "expr": "rate(hauntedai_agent_failures_total{agent_type=\"deploy\"}[5m])",
            "legendFormat": "Deploy Failures"
          }
        ],
        "yAxes": [
          {
            "label": "Rate/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 9, "w": 6, "x": 18, "y": 17}
      }
    ]
  }
}